{
    "name": "zachleigh/laravel-property-bag",
    "description": "Easy Laravel user settings using a property bag",
    "type": "project",
    "keywords": ["laravel", "user", "settings", "property", "user settings", "propety bag"],
    "homepage": "https://github.com/zachleigh/laravel-property-bag",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Leigh",
            "email": "zachleigh@fastmail.jp"
        }
    ],

    "require": {
        "php": ">=5.5.9"
    },

    "require-dev": {
        "laravel/laravel": "~5.5",
        "laravel/browser-kit-testing": "~1.0",
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "LaravelPropertyBag\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "LaravelPropertyBag\\tests\\": "tests/"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra":{
        "laravel":{
            "providers":[
                "LaravelPropertyBag\\ServiceProvider"
            ]
        }
    }
}
